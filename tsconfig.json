{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@images/*": [
        "src/images/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@scripts/*": [
        "src/scripts/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ]
    }
  },
  "include": [
    "src",
    "astro-imagetools.d.ts"
  ]
}
